<%= simple_form_for(@submission) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :languageUsed %>
    <%= f.input :submissionFile %>
    <%= f.association :problem %>
    <%= f.association :contest %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
