<h1>Submissions</h1>

<table>
  <thead>
    <tr>
      <th>Submission ID</th>
      <th>Problem ID</th>
      <th>Languageused</th>
      <th>Date Time Of Submission</th>
      <th>Time</th>
      <th>Memory Used</th>
      <th>Status</th>
      <th>User</th>
      <th>Contest</th>
    </tr>
  </thead>

  <tbody>
    <% @submissions.each do |submission| %>
      <tr>
        <td><%= submission.id %></td>
        <td><%= submission.problem %></td>
        <td><%= submission.languageUsed %></td>
        <td><%= submission.problem %></td>
        <td><%= submission.dateTimeOfSubmission %></td>
        <td><%= submission.timeTaken %></td>
        <td><%= submission.memoryUsed %></td>
        <td><%= submission.status %></td>
        <td><%= submission.user %></td>
        <td><%= submission.contest %></td>
<!--         <td><%= link_to 'Show', submission %></td>
        <td><%= link_to 'Edit', edit_submission_path(submission) %></td>
        <td><%= link_to 'Destroy', submission, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>

<br>

