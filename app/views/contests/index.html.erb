<h1>Listing contests</h1>

<table>
  <thead>
    <tr>
      <th>Contestname</th>
      <th>Startdatetime</th>
      <th>Enddatetime</th>
      <th>Conteststatus</th>
      <th>User</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @contests.each do |contest| %>
      <tr>
        <td><%= contest.contestName %></td>
        <td><%= contest.startDateTime %></td>
        <td><%= contest.endDateTime %></td>
        <td><%= contest.contestStatus %></td>
        <td><%= contest.user %></td>
        <td><%= link_to 'Show', contest %></td>
        <td><%= link_to 'Edit', edit_contest_path(contest) %></td>
        <td><%= link_to 'Destroy', contest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contest', new_contest_path %>
