<%= simple_form_for(@contest) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :contestName %>
    <%= f.input :startDateTime %>
    <%= f.input :endDateTime %>
    <%= f.input :contestStatus %>
    <%= f.association :user %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
